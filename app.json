{
    "expo": {
        "name": "Bada Student",
        "slug": "bada-student",
        "version": "1.0.0",
        "orientation": "default",
        "userInterfaceStyle": "light",
        "icon": "./assets/icon.png",
        "splash": {
            "image": "./assets/splash.png",
            "resizeMode": "cover",
            "backgroundColor": "#100D25"
        },
        "updates": {
            "fallbackToCacheTimeout": 0
        },
        "assetBundlePatterns": ["**/*"],
        "ios": {
            "buildNumber": "1",
            "bundleIdentifier": "com.zetasolutionsonline.bada",
            "supportsTablet": true,
            "infoPlist": {
                "CFBundleAllowMixedLocalizations": true
            }
        },
        "android": {
            "versionCode": 2,
            "softwareKeyboardLayoutMode": "resize",
            "package": "com.zetasolutionsonline.bada",
            "jsEngine": "hermes"
        },
        "web": {
            "favicon": "./assets/icon.png"
        },
        "plugins": ["sentry-expo"],
        "extra": {
            "eas": {}
        }
    }
}
